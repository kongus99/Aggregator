# Routes
# This file defines all application routes (Higher priority routes first)
# ~~~~

# An example controller showing a sample home page
GET        /                    controllers.HomeController.main
GET        /gogData             controllers.HomeController.gogData
GET        /steamData           controllers.HomeController.steamData
GET        /allData             controllers.HomeController.allData

GET        /comparison          controllers.ComparisonController.main
GET        /comparison/data     controllers.ComparisonController.data


# Map static resources from the /public folder to the /assets URL path
GET        /assets/*file        controllers.Assets.versioned(path="/public", file: Asset)
